{

    "manifest_version": 2,
    "name": "Varnam Web Extension",
    "version": "1.0",

    "description": "Transliterate Malaylam to English",
    "permissions": [
        "<all_urls>",
        "tabs"
    ],


    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": [
            "src/js/editor_transliterate.js",
            "src/js/tribute.js",
            "src/js/loader.js"

        ],
        "css": [
            "src/css/tribute.css"
        ],
        "run_at": "document_end"
    }]

}